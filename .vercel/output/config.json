{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/chat/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/api-reference/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/code-examples/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/getting-started/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/work/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}